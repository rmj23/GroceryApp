{
  "name": "@grocery-app/contracts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:types": "openapi-typescript ./openapi.json -o ./src/types.ts",
    "generate:client": "openapi-typescript-codegen --input ./openapi.json --output ./src/client --client axios",
    "generate": "npm run generate:types && npm run generate:client"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "openapi-typescript": "^7.6.1",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "^5.7.2"
  }
}


